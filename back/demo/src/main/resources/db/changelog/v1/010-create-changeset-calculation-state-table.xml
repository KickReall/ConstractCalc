<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="create-calculation-state-table" author="barakovskaya_elena">
        <createTable tableName="calculation_state" remarks="Состояния рассчетов">
            <column name="id" type="int" autoIncrement="true" remarks="ID статуса">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar" remarks="Название статуса">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>