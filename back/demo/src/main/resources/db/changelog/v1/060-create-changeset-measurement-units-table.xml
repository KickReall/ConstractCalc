<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
<changeSet id="create-measurement-units-table" author="barakovskaya_elena">
    <createTable tableName="measurement_units" remarks="Единицы измерения">
        <column name="id" type="int" autoIncrement="true" remarks="ID единицы измерения">
            <constraints primaryKey="true" nullable="false"/>
        </column>
        <column name="measurement_units_name" type="varchar(20)">
            <constraints nullable="false"/>
        </column>
    </createTable>
</changeSet>
</databaseChangeLog>